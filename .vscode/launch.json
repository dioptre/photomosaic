    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Mosaid Test",
            "type": "debugpy",
            "request": "launch",
            "program": "mosaic.py",
            "console": "integratedTerminal",
            "args": "--target media/example/image_0001.jpg --savepath \"media/output/%s-mosaic-scale-%d.jpg\" --codebook-dir media/faces/other_faces --scale 0.01  --height-aspect 592 --width-aspect 896"
        }
    ]
}